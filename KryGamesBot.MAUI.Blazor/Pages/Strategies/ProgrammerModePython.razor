@inherits ProgrammerModeLUA

@{
    base.BuildRenderTree(__builder);
}

@code {
    public ProgrammerModePython():base()
    {
        SetLanguage();
    }
    protected override void SetLanguage()
    {
        LanguageName = "python";
        FileExtension = "py";
        TemplateName = "PYTemplate.py";
        DisplayName = "Python";
    }
}

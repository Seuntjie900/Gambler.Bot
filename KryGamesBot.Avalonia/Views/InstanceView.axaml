<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="KryGamesBot.Avalonia.Views.InstanceView" 
			 x:DataType="vm:InstanceViewModel"
			 xmlns:vm="using:KryGamesBot.Avalonia.ViewModels"
			 xmlns:common="using:KryGamesBot.Avalonia.Views.Common"
			 >
	<UserControl.DataContext>
		<vm:InstanceViewModel/>
	</UserControl.DataContext>
	<Grid>
		<common:SelectSiteView DataContext="{Binding SelectSite}"   />
	</Grid>
</UserControl>
